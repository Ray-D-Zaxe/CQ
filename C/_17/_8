#include <stdio.h>
int main()
{
    int a, i, j, c = 0;
    scanf("%d\n", &a);
    int b[a][a];
    for(i = 0; i < a; i++){
        for(j = 0; j < a; j++)
            scanf("%d ", &b[i][j]);
        printf("\n");
    }
    for(i = 0; i < a; i++)
        c += b[i][i];
    printf("%d", c);
    return 0;
}